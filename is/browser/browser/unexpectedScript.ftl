unexpected-script-close-button =
    .aria-label = Loka
# This string is used in the notification bar
# Variables:
#   $origin (string) - The top level domain the unexpected script was loaded from
unexpected-script-load-message = <strong>{ -brand-short-name } hefur greint óvænta skriftu með auknar heimildir frá { $origin } </strong>
unexpected-script-load-message-button-allow =
    .label = Leyfa
    .accesskey = a
unexpected-script-load-message-button-block =
    .label = Loka á
    .accesskey = k
unexpected-script-load-title = Óvænt innhleðsla skriftu
unexpected-script-load-email-textbox =
    .placeholder = Settu hér inn tölvupóstfang
    .aria-label = Settu hér inn tölvupóstfang
unexpected-script-load-more-info = Nánari upplýsingar
unexpected-script-load-learn-more = Kanna nánar
unexpected-script-load-telemetry-disabled = Skýrslugerð er óvirk því fjarmælingar eru óvirkar í stillingum. Virkjaðu fjarmælingar til að senda inn skýrslu.
