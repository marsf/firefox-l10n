unexpected-script-close-button =
    .aria-label = Začinić
# This string is used in the notification bar
# Variables:
#   $origin (string) - The top level domain the unexpected script was loaded from
unexpected-script-load-message = <strong>{ -brand-short-name } je njewotčakowany, priwilegowany skript wot { $origin } wotkrył</strong>
unexpected-script-load-message-button-allow =
    .label = Dowolić
    .accesskey = D
unexpected-script-load-message-button-block =
    .label = Blokować
    .accesskey = B
unexpected-script-load-title = Njewočakowane skriptowe začitanje
