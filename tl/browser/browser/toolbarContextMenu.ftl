#   $tabCount (number) - Number of tabs
toolbar-context-menu-reopen-closed-tabs =
    .label =
        { $tabCount ->
            [1] Muling Buksan ang Naisarang Tab
            [one] Muling Buksan ang Naisarang Tab
           *[other] Muling Buksan ang mga Naisarang Tab
        }
    .accesskey = M
